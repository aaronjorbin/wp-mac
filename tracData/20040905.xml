if ( substr($self, -15) == substr($item&lt;a class="changeset" href="http://core.trac.wordpress.org/changeset/2" title="*** empty log message ***
"&gt;[2]&lt;/a&gt;, -15) ) $class = ' class="current"';
&lt;/p&gt;
&lt;/blockquote&gt;
&lt;p&gt;
OR change the function :
&lt;/p&gt;
&lt;blockquote&gt;
&lt;p&gt;
if (  strcmp($self,$item&lt;a class="changeset" href="http://core.trac.wordpress.org/changeset/2" title="*** empty log message ***
"&gt;[2]&lt;/a&gt;) == 0  ) $class = ' class="current"';
&lt;/p&gt;
&lt;/blockquote&gt;
</description>
